<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
		<record id="view_prestashop_backend_form_inherit" model="ir.ui.view">
	    	<field name="name">prestashop.backend.form.inherit</field>
	    	<field name="model">prestashop.backend</field>
	    	<field name="inherit_id" ref="prestashoperpconnect.view_prestashop_backend_form"/>
	    	<field name="arch" type="xml">
	    		<xpath expr="//notebook/page[@string='Imports']/group[last()]" position="after">
	    			<group>
	                    <label string="Import warehouse" class="oe_inline"/>
	                    <div>
	                        <button name="import_warehouses"
	                                type="object"
	                                class="oe_highlight"
	                                string="Import in background"/>
	                    </div>
	    			</group>
	    		</xpath>
	    	</field>
		</record>
	</data>
</openerp>