<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data>
	<record id="view_prestashop_backend_form" model="ir.ui.view">
    	<field name="name">prestashop.backend.form</field>
    	<field name="model">prestashop.backend</field>
    	<field name="inherit_id" ref="prestashoperpconnect.view_prestashop_backend_form"/>
    	<field name="arch" type="xml">
    		<xpath expr="//field[@name='warehouse_id']" position="replace">
    			<group>
    				<field name="warehouse_id" attrs="{'required': [('use_stock_by_shop', '=', False)]}"/>
    				<field name="use_stock_by_shop"/>
    			</group>
    		</xpath>
        </field>
    </record>
</data>
</openerp>